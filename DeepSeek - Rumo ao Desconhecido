<span class="token keyword">interface</span> <span class="token class-name">PlatformConnection</span> <span class="token punctuation">{</span>
  web<span class="token operator">:</span> WebClient<span class="token punctuation">;</span>
  desktop<span class="token operator">:</span> ElectronApp<span class="token punctuation">;</span> 
  mobile<span class="token operator">:</span> ReactNativeApp<span class="token punctuation">;</span>
  
  <span class="token comment">// Todos conectam ao mesmo backend</span>
