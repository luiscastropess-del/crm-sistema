require('dotenv').config();

async function testConnection() {
  console.log('üîç Testando conex√£o com PostgreSQL...');
  console.log('üìù Connection string:', process.env.DATABASE_URL);

  if (!process.env.DATABASE_URL) {
    console.error('‚ùå DATABASE_URL n√£o encontrada no .env');
    process.exit(1);
  }

  const pool = new Pool({
    connectionString: process.env.DATABASE_URL,
    ssl: { 
      rejectUnauthorized: false 
    },
    connectionTimeoutMillis: 10000,
  });

  try {
    console.log('‚ö° Tentando conectar...');
    const client = await pool.connect();
    console.log('‚úÖ Conex√£o estabelecida com sucesso!');
    
    // Testa uma query simples
    const result = await client.query('SELECT version() as postgres_version, NOW() as current_time');
    console.log('üìä PostgreSQL Version:', result.rows[0].postgres_version.split(',')[0]);
    console.log('‚è∞ Server Time:', result.rows[0].current_time);
    
    // Testa se podemos criar tabelas
    console.log('üß™ Testando cria√ß√£o de tabela...');
    await client.query('CREATE TABLE IF NOT EXISTS connection_test (id SERIAL PRIMARY KEY, test_time TIMESTAMP)');
    await client.query('INSERT INTO connection_test (test_time) VALUES (NOW())');
    console.log('‚úÖ Teste de escrita funcionando!');
    
    client.release();
    await pool.end();
    console.log('üéâ Todos os testes passaram! PostgreSQL est√° pronto.');
    process.exit(0);
  } catch (error) {
    console.error('‚ùå Erro na conex√£o:', error.message);
    console.log('\\nüîß Diagn√≥stico:');
    console.log('   Connection String:', process.env.DATABASE_URL);
    console.log('   Erro Code:', error.code);
    console.log('\\nüí° Solu√ß√µes:');
    console.log('   1. Verifique DATABASE_URL no .env');
    console.log('   2. Confirme que o Neon.tech est√° ativo');
    console.log('   3. Teste no site do Neon.tech');
    console.log('   4. Verifique firewall/rede');
    process.exit(1);
  }
}

