    <span class="token punctuation">}</span>
  <span class="token punctuation">}</span>

  <span class="token comment">// Cria arquivo com conte√∫do</span>
  <span class="token function">createFile</span><span class="token punctuation">(</span><span class="token parameter">filePath<span class="token punctuation">,</span> content</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>
    <span class="token keyword">this</span><span class="token punctuation">.</span><span class="token function">createDir</span><span class="token punctuation">(</span>path<span class="token punctuation">.</span><span class="token function">dirname</span><span class="token punctuation">(</span>filePath<span class="token punctuation">)</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
