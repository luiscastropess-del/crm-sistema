    console<span class="token punctuation">.</span><span class="token function">log</span><span class="token punctuation">(</span><span class="token string">'‚úÖ Migra√ß√£o inicial criada!'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
  <span class="token punctuation">}</span>

  <span class="token comment">// Cria script para executar migra√ß√£o</span>
  <span class="token function">createMigrationRunner</span><span class="token punctuation">(</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>
    console<span class="token punctuation">.</span><span class="token function">log</span><span class="token punctuation">(</span><span class="token string">'\n‚ö° Criando executor de migra√ß√µes...'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> migrationRunnerPath <span class="token operator">=</span> path<span class="token punctuation">.</span><span class="token function">join</span><span class="token punctuation">(</span><span class="token keyword">this</span><span class="token punctuation">.</span>projectDir<span class="token punctuation">,</span> <span class="token string">'server/src/migrations/run-migrations.ts'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> runnerContent <span class="token operator">=</span> <span class="token template-string"><span class="token template-punctuation string">`</span><span class="token string">import { database } from '../config/database';
import fs from 'fs';
import path from 'path';

class MigrationRunner {
  private migrationsPath: string;

  constructor() {
    this.migrationsPath = path.join(__dirname);
  }

  async runMigrations() {
    try {
      console.log('üöÄ Executando migra√ß√µes...');
      
      // Ordena os arquivos de migra√ß√£o
      const files = fs.readdirSync(this.migrationsPath)
        .filter(file =&gt; file.endsWith('.sql'))
        .sort();

      for (const file of files) {
        console.log(\`üìÑ Executando migra√ß√£o: \${file}\`);
        
        const migrationSQL = fs.readFileSync(
          path.join(this.migrationsPath, file), 
          'utf8'
        );

        await database.query(migrationSQL);
        console.log(\`‚úÖ Migra√ß√£o \${file} executada com sucesso\`);
      }

      console.log('üéâ Todas as migra√ß√µes executadas!');
    } catch (error) {
      console.error('‚ùå Erro ao executar migra√ß√µes:', error);
      throw error;
    }
  }

  async checkMigrations() {
    try {
      // Verifica se a tabela companies existe (indicador de migra√ß√£o)
      await database.query('SELECT 1 FROM companies LIMIT 1');
      console.log('‚úÖ Migra√ß√µes j√° aplicadas');
      return true;
    } catch (error) {
      console.log('üìã Migra√ß√µes pendentes');
      return false;
    }
  }
}

const migrationRunner = new MigrationRunner();

// Executa se chamado diretamente
if (require.main === module) {
  migrationRunner.runMigrations()
    .then(() =&gt; process.exit(0))
    .catch(() =&gt; process.exit(1));
}

