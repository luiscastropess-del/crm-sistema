    res.json({ 
      status: 'OK', 
      timestamp: new Date().toISOString(),
      version: '1.0.0',
      database: dbStatus ? 'CONECTADO' : 'ERRO',
      environment: process.env.NODE_ENV
    });
  } catch (error) {
    res.status(500).json({
      status: 'ERROR',
      database: 'ERRO',
      error: error.message
    });
  }
});

