import fs from 'fs';
import path from 'path';

class MigrationRunner {
  private migrationsPath: string;

  constructor() {
    this.migrationsPath = path.join(__dirname);
  }

  async runMigrations() {
    try {
      console.log('üöÄ Executando migra√ß√µes...');
      
      // Ordena os arquivos de migra√ß√£o
      const files = fs.readdirSync(this.migrationsPath)
        .filter(file =&gt; file.endsWith('.sql'))
        .sort();

      for (const file of files) {
        console.log(\`üìÑ Executando migra√ß√£o: \${file}\`);
        
        const migrationSQL = fs.readFileSync(
          path.join(this.migrationsPath, file), 
          'utf8'
        );

        await database.query(migrationSQL);
        console.log(\`‚úÖ Migra√ß√£o \${file} executada com sucesso\`);
      }

      console.log('üéâ Todas as migra√ß√µes executadas!');
    } catch (error) {
      console.error('‚ùå Erro ao executar migra√ß√µes:', error);
      throw error;
    }
  }

  async checkMigrations() {
    try {
      // Verifica se a tabela companies existe (indicador de migra√ß√£o)
      await database.query('SELECT 1 FROM companies LIMIT 1');
      console.log('‚úÖ Migra√ß√µes j√° aplicadas');
      return true;
    } catch (error) {
      console.log('üìã Migra√ß√µes pendentes');
      return false;
    }
  }
}

const migrationRunner = new MigrationRunner();

// Executa se chamado diretamente
if (require.main === module) {
  migrationRunner.runMigrations()
    .then(() =&gt; process.exit(0))
    .catch(() =&gt; process.exit(1));
}

export { migrationRunner };</span><span class="token template-punctuation string">`</span></span><span class="token punctuation">;</span>

