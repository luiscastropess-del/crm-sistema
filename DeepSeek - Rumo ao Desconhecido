    <span class="token keyword">const</span> jsDbPath <span class="token operator">=</span> path<span class="token punctuation">.</span><span class="token function">join</span><span class="token punctuation">(</span><span class="token keyword">this</span><span class="token punctuation">.</span>projectDir<span class="token punctuation">,</span> <span class="token string">'server/src/config/database.js'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> jsDbContent <span class="token operator">=</span> <span class="token template-string"><span class="token template-punctuation string">`</span><span class="token string">const { Pool } = require('pg');

class Database {
  constructor() {
    this.pool = new Pool({
      connectionString: process.env.DATABASE_URL,
      ssl: process.env.NODE_ENV === 'production' ? { rejectUnauthorized: false } : false,
      max: 20,
      idleTimeoutMillis: 30000,
      connectionTimeoutMillis: 2000,
    });

    this.setupEventListeners();
  }

  setupEventListeners() {
    this.pool.on('connect', () =&gt; {
      console.log('üîó Nova conex√£o com PostgreSQL estabelecida');
    });

    this.pool.on('error', (err) =&gt; {
      console.error('‚ùå Erro inesperado na conex√£o com PostgreSQL:', err);
    });
  }

  async connect() {
    try {
      const client = await this.pool.connect();
      console.log('‚úÖ Conectado ao PostgreSQL com sucesso');
      client.release();
      return true;
    } catch (error) {
      console.error('‚ùå Erro ao conectar com PostgreSQL:', error.message);
      throw error;
    }
  }

  async checkConnection() {
    try {
      const client = await this.pool.connect();
      await client.query('SELECT 1');
      client.release();
      return true;
    } catch (error) {
      return false;
    }
  }

  async query(text, params = []) {
    const start = Date.now();
    try {
      const result = await this.pool.query(text, params);
      const duration = Date.now() - start;
      console.log(\`üìä Query executada em \${duration}ms: \${text}\`);
      return result;
    } catch (error) {
      console.error('‚ùå Erro na query:', { text, error: error.message });
      throw error;
    }
  }

  async transaction(callback) {
    const client = await this.pool.connect();
    try {
      await client.query('BEGIN');
      const result = await callback(client);
      await client.query('COMMIT');
      return result;
    } catch (error) {
      await client.query('ROLLBACK');
      throw error;
    } finally {
      client.release();
    }
  }

  getPool() {
    return this.pool;
  }
}

module.exports = { database: new Database() };</span><span class="token template-punctuation string">`</span></span><span class="token punctuation">;</span>

