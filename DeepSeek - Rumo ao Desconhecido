  <span class="token punctuation">}</span>

  <span class="token comment">// Cria um script de migra√ß√£o mais robusto</span>
  <span class="token function">createRobustMigration</span><span class="token punctuation">(</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>
    console<span class="token punctuation">.</span><span class="token function">log</span><span class="token punctuation">(</span><span class="token string">'\n‚ö° Criando migra√ß√£o robusta...'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> robustMigrationPath <span class="token operator">=</span> path<span class="token punctuation">.</span><span class="token function">join</span><span class="token punctuation">(</span><span class="token keyword">this</span><span class="token punctuation">.</span>projectDir<span class="token punctuation">,</span> <span class="token string">'server/src/migrations/run-migrations-robust.js'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> robustMigration <span class="token operator">=</span> <span class="token template-string"><span class="token template-punctuation string">`</span><span class="token string">const { Pool } = require('pg');
require('dotenv').config();
const fs = require('fs');
const path = require('path');

class RobustMigrationRunner {
  constructor() {
    this.migrationsPath = path.join(__dirname);
    this.pool = new Pool({
      connectionString: process.env.DATABASE_URL,
      ssl: { rejectUnauthorized: false },
      connectionTimeoutMillis: 15000,
    });
  }

  async testConnection() {
    try {
      console.log('üîç Testando conex√£o com o banco...');
      const client = await this.pool.connect();
      const result = await client.query('SELECT 1 as test');
      client.release();
      console.log('‚úÖ Conex√£o com PostgreSQL estabelecida!');
      return true;
    } catch (error) {
      console.error('‚ùå Falha na conex√£o:', error.message);
      console.log('üìù Connection string usada:', process.env.DATABASE_URL);
      return false;
    }
  }

  async runMigrations() {
    console.log('üöÄ Iniciando migra√ß√µes...');
    
    // Testa conex√£o primeiro
    const canConnect = await this.testConnection();
    if (!canConnect) {
      console.log('üí° Dicas para resolver:');
      console.log('   1. Verifique DATABASE_URL no .env');
      console.log('   2. Confirme que o Neon.tech est√° ativo');
      console.log('   3. Teste no site do Neon.tech: https://console.neon.tech');
      console.log('   4. Verifique se a senha est√° correta');
      process.exit(1);
    }

    try {
      // Ordena os arquivos de migra√ß√£o
      const files = fs.readdirSync(this.migrationsPath)
        .filter(file =&gt; file.endsWith('.sql'))
        .sort();

      for (const file of files) {
        console.log(\`\\nüìÑ Executando: \${file}\`);
        
        const migrationSQL = fs.readFileSync(
          path.join(this.migrationsPath, file), 
          'utf8'
        );

        await this.pool.query(migrationSQL);
        console.log(\`‚úÖ \${file} - OK\`);
      }

      console.log('\\nüéâ Todas as migra√ß√µes executadas com sucesso!');
      
      // Mostra o que foi criado
      await this.showCreatedTables();
      
    } catch (error) {
      console.error('‚ùå Erro durante migra√ß√£o:', error);
      process.exit(1);
    } finally {
      await this.pool.end();
    }
  }

  async showCreatedTables() {
    try {
      const result = await this.pool.query(\`
        SELECT table_name 
        FROM information_schema.tables 
        WHERE table_schema = 'public'
        ORDER BY table_name
      \`);
      
      console.log('\\nüìä Tabelas criadas no banco:');
      result.rows.forEach(row =&gt; {
        console.log(\`   ‚úÖ \${row.table_name}\`);
      });
    } catch (error) {
      console.log('‚ö†Ô∏è N√£o foi poss√≠vel listar tabelas:', error.message);
    }
  }
}

