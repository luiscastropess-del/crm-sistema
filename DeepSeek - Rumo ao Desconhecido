      <span class="token punctuation">}</span><span class="token punctuation">;</span>

      packageJson<span class="token punctuation">.</span>devDependencies <span class="token operator">=</span> <span class="token punctuation">{</span>
        <span class="token operator">...</span>packageJson<span class="token punctuation">.</span>devDependencies<span class="token punctuation">,</span>
