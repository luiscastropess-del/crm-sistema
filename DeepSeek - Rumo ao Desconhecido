require('dotenv').config();

class Database {
  constructor() {
    console.log('üîß Inicializando conex√£o PostgreSQL...');
    console.log('üìù Host:', process.env.DATABASE_URL?.split('@')[1]?.split('/')[0]);
    
    this.pool = new Pool({
      connectionString: process.env.DATABASE_URL,
      ssl: { 
        rejectUnauthorized: false 
      },
      max: 20,
      idleTimeoutMillis: 30000,
      connectionTimeoutMillis: 10000,
    });

    this.setupEventListeners();
  }

  setupEventListeners() {
    this.pool.on('connect', () =&gt; {
      console.log('üîó Nova conex√£o PostgreSQL estabelecida');
    });

    this.pool.on('error', (err) =&gt; {
      console.error('‚ùå Erro inesperado no PostgreSQL:', err);
    });
  }

  async connect() {
    try {
      console.log('‚ö° Conectando ao PostgreSQL...');
      const client = await this.pool.connect();
      console.log('‚úÖ Conectado ao PostgreSQL com sucesso');
      
      // Testa a conex√£o
      await client.query('SELECT 1');
      client.release();
      
      return true;
    } catch (error) {
      console.error('‚ùå Erro ao conectar com PostgreSQL:');
      console.error('   Mensagem:', error.message);
      console.error('   Code:', error.code);
      console.error('   Host:', process.env.DATABASE_URL?.split('@')[1]?.split('/')[0]);
      throw error;
    }
  }

  async checkConnection() {
    try {
      const client = await this.pool.connect();
      await client.query('SELECT 1');
      client.release();
      return true;
    } catch (error) {
      console.error('‚ùå Check connection failed:', error.message);
      return false;
    }
  }

  async query(text, params = []) {
    const start = Date.now();
    try {
      const result = await this.pool.query(text, params);
      const duration = Date.now() - start;
      console.log(\`üìä Query \${duration}ms: \${text.substring(0, 50)}...\`);
      return result;
    } catch (error) {
      console.error('‚ùå Erro na query:', { 
        text: text.substring(0, 100), 
        error: error.message 
      });
      throw error;
    }
  }

  async transaction(callback) {
    const client = await this.pool.connect();
    try {
      await client.query('BEGIN');
      const result = await callback(client);
      await client.query('COMMIT');
      return result;
    } catch (error) {
      await client.query('ROLLBACK');
      throw error;
    } finally {
      client.release();
    }
  }

  getPool() {
    return this.pool;
  }
}

module.exports = { database: new Database() };</span><span class="token template-punctuation string">`</span></span><span class="token punctuation">;</span>

