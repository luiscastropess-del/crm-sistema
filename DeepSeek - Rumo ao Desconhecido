    console<span class="token punctuation">.</span><span class="token function">log</span><span class="token punctuation">(</span><span class="token string">'‚úÖ Configura√ß√£o do database atualizada!'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
  <span class="token punctuation">}</span>

  <span class="token comment">// Cria migra√ß√£o robusta no SERVER</span>
  <span class="token function">createRobustMigration</span><span class="token punctuation">(</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>
    console<span class="token punctuation">.</span><span class="token function">log</span><span class="token punctuation">(</span><span class="token string">'\n‚ö° Criando migra√ß√£o robusta no server...'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> robustMigrationPath <span class="token operator">=</span> path<span class="token punctuation">.</span><span class="token function">join</span><span class="token punctuation">(</span><span class="token keyword">this</span><span class="token punctuation">.</span>serverDir<span class="token punctuation">,</span> <span class="token string">'run-migrations.js'</span><span class="token punctuation">)</span><span class="token punctuation">;</span>
    
    <span class="token keyword">const</span> robustMigration <span class="token operator">=</span> <span class="token template-string"><span class="token template-punctuation string">`</span><span class="token string">const { Pool } = require('pg');
require('dotenv').config();
const fs = require('fs');
const path = require('path');

class RobustMigrationRunner {
  constructor() {
    this.migrationsPath = path.join(__dirname, 'src/migrations');
    console.log('üîß Inicializando migra√ß√µes...');
    
    this.pool = new Pool({
      connectionString: process.env.DATABASE_URL,
      ssl: { rejectUnauthorized: false },
      connectionTimeoutMillis: 15000,
    });
  }

  async testConnection() {
    try {
      console.log('üîç Testando conex√£o com o banco...');
      console.log('üìù Usando:', process.env.DATABASE_URL?.split('@')[1]?.split('/')[0]);
      
      const client = await this.pool.connect();
      const result = await client.query('SELECT version() as version');
      client.release();
      
      console.log('‚úÖ Conex√£o estabelecida!');
      console.log('üêò PostgreSQL:', result.rows[0].version.split(',')[0]);
      return true;
    } catch (error) {
      console.error('‚ùå Falha na conex√£o:');
      console.error('   Erro:', error.message);
      console.error('   Code:', error.code);
      console.error('   Host:', process.env.DATABASE_URL);
      return false;
    }
  }

  async runMigrations() {
    console.log('üöÄ Iniciando migra√ß√µes do CRM...');
    
    // Testa conex√£o primeiro
    const canConnect = await this.testConnection();
    if (!canConnect) {
      console.log('\\nüí° Dicas para resolver:');
      console.log('   1. Verifique DATABASE_URL no .env');
      console.log('   2. Confirme que o Neon.tech est√° ativo');
      console.log('   3. Teste em: https://console.neon.tech');
      console.log('   4. Verifique usu√°rio/senha');
      process.exit(1);
    }

    try {
      // Ordena os arquivos de migra√ß√£o
      const files = fs.readdirSync(this.migrationsPath)
        .filter(file =&gt; file.endsWith('.sql'))
        .sort();

      console.log(\`üìÅ Encontradas \${files.length} migra√ß√µes\\n\`);

      for (const file of files) {
        console.log(\`üìÑ Executando: \${file}\`);
        
        const migrationSQL = fs.readFileSync(
          path.join(this.migrationsPath, file), 
          'utf8'
        );

        await this.pool.query(migrationSQL);
        console.log(\`‚úÖ \${file} - OK\`);
      }

      console.log('\\nüéâ Todas as migra√ß√µes executadas!');
      
      // Mostra o que foi criado
      await this.showCreatedTables();
      
    } catch (error) {
      console.error('‚ùå Erro durante migra√ß√£o:', error);
      process.exit(1);
    } finally {
      await this.pool.end();
    }
  }

  async showCreatedTables() {
    try {
      const result = await this.pool.query(\`
        SELECT table_name 
        FROM information_schema.tables 
        WHERE table_schema = 'public'
        ORDER BY table_name
      \`);
      
      console.log('\\nüìä Tabelas no banco:');
      result.rows.forEach(row =&gt; {
        console.log(\`   ‚úÖ \${row.table_name}\`);
      });
      
      console.log('\\nüöÄ Banco de dados pronto para uso!');
    } catch (error) {
      console.log('‚ö†Ô∏è N√£o foi poss√≠vel listar tabelas:', error.message);
    }
  }
}

// Executa as migra√ß√µes
new RobustMigrationRunner().runMigrations();</span><span class="token template-punctuation string">`</span></span><span class="token punctuation">;</span>

