import cors from 'cors';
import helmet from 'helmet';
import dotenv from 'dotenv';
import { createServer } from 'http';
import { Server as SocketIOServer } from 'socket.io';
import { database } from './config/database';

// Carrega vari√°veis de ambiente
dotenv.config();

const app = express();
const server = createServer(app);
const io = new SocketIOServer(server, {
  cors: {
    origin: "*",
    methods: ["GET", "POST"]
  }
});

// Conecta ao banco de dados
database.connect()
  .then(() =&gt; console.log('‚úÖ Conectado ao PostgreSQL'))
  .catch(err =&gt; console.error('‚ùå Erro de conex√£o com PostgreSQL:', err));

// Middlewares
app.use(helmet());
app.use(cors());
app.use(express.json({ limit: '10mb' }));

// Health check com status do banco
app.get('/health', async (req, res) =&gt; {
  try {
    const dbStatus = await database.checkConnection();
    res.json({ 
      status: 'OK', 
      timestamp: new Date().toISOString(),
      version: '1.0.0',
      database: dbStatus ? 'CONECTADO' : 'ERRO',
      environment: process.env.NODE_ENV
    });
  } catch (error) {
    res.status(500).json({
      status: 'ERROR',
      database: 'ERRO',
      error: error.message
    });
  }
});

// Rota de teste do banco
app.get('/api/db-test', async (req, res) =&gt; {
  try {
    const result = await database.query('SELECT NOW() as current_time, version() as postgres_version');
    res.json({
      success: true,
      message: 'Conex√£o com PostgreSQL funcionando!',
      data: result.rows[0]
    });
  } catch (error) {
    res.status(500).json({
      success: false,
      error: 'Erro na conex√£o com o banco',
      details: error.message
    });
  }
});

// Socket.io para tempo real
io.on('connection', (socket) =&gt; {
  console.log('üîå Cliente conectado:', socket.id);
  
  socket.on('disconnect', () =&gt; {
    console.log('üîå Cliente desconectado:', socket.id);
  });
});

const PORT = process.env.PORT || 3001;
server.listen(PORT, () =&gt; {
  console.log(\`üöÄ Servidor CRM rodando na porta \${PORT}\`);
  console.log(\`üìä Health check: http://localhost:\${PORT}/health\`);
  console.log(\`üóÉÔ∏è  Teste do BD: http://localhost:\${PORT}/api/db-test\`);
});

export { app, io };</span><span class="token template-punctuation string">`</span></span><span class="token punctuation">;</span>

